<div class="blur-box text" style="margin-bottom: 100px; margin-left: 620px">
  <div >
    <table>
      <tr><td colspan="2">  <h1 style="font-size: 60px"> <%= t(:'.all_users') %> </h1></td></tr>
      <tr>
        <th><%= t(:'.email') %></th>
        <th> <%= t(:'.solved') %> </th>
      </tr>
      <% ResultsForUser.reorder('time desc').all.each do |row| %>
        <tr style="font-size: 24px">
          <td> <p> <%= row.user %> </p> </td>
          <td style="font-size: 24px"> <%= row.time %></td>
        </tr>
      <% end %>
    </table>
  </div>
</div>